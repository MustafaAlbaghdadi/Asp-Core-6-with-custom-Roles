<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>aspnet-aspcore-29A96A75-80A1-4D4F-9304-A615B1AEB45C</UserSecretsId>
    <RootNamespace>aspcore</RootNamespace>
    <AssemblyName>aspcore</AssemblyName>
